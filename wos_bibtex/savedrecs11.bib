@article{ ISI:000379611700011,
Author = {Liu, Joseph T. C.},
Title = {{Some fundamental considerations of streamwise vortices found useful in
   mixing enhancement and jet noise suppression}},
Journal = {{INTERNATIONAL JOURNAL OF AEROACOUSTICS}},
Year = {{2016}},
Volume = {{15}},
Number = {{4-5}},
Pages = {{515-525}},
Month = {{JUL}},
Abstract = {{The present contribution addresses the possibility of extracting more
   jet noise suppression capabilities of stationary chevron nozzle exits
   through unsteady excitation of secondary instabilities. An example of
   insertion of steady streamwise vorticity into a mixing region is given
   to illustrate its effectiveness in promoting enhanced mixing. Using
   stability theory as a guide, unsteady secondary instability of the
   streamwise vorticity is excited with emphasis on nonlinear effects. The
