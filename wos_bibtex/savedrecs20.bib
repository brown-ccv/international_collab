@article{ ISI:000370700500015,
Author = {Singer, Mervyn and Deutschman, Clifford S. and Seymour, Christopher
   Warren and Shankar-Hari, Manu and Annane, Djillali and Bauer, Michael
   and Bellomo, Rinaldo and Bernard, Gordon R. and Chiche, Jean-Daniel and
