@article{ ISI:000373230900007,
Author = {Jiao, Qiang and Wei, Lei and Chen, Chongwei and Li, Pengcui and Wang,
   Xiaohu and Li, Yongping and Guo, Li and Zhang, Congming and Wei,
   Xiaochun},
Title = {{Cartilage oligomeric matrix protein and hyaluronic acid are sensitive
   serum biomarkers for early cartilage lesions in the knee joint}},
Journal = {{BIOMARKERS}},
Year = {{2016}},
Volume = {{21}},
Number = {{2}},
Pages = {{146-151}},
Month = {{FEB 17}},
Abstract = {{The purpose of this study was to evaluate the relationship between five
   previously established serum osteoarthritis biomarkers and the severity
   of cartilage lesions in the knee. Cartilage damage (classified according
   to the Outerbridge scoring system) and serum concentrations of cartilage
   oligomeric matrix protein (COMP), collagen type II C-telopeptide
   (CTX-II), matrix metalloproteinase-3 (MMP-3), collagen type III
   N-propeptide, (PIIINP), and hyaluronic acid (HA) were determined in 79
   patients who underwent knee arthroscopy or total knee replacement. HA
   and COMP concentrations were significantly higher in the Outerbridge
   score 1 and 2 groups, respectively. These results suggest that serum
   COMP and HA concentrations can be used to predict early cartilage
   lesions in the knee.}},
