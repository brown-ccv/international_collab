@article{ ISI:000381844100012,
Author = {Braverman, Alexander and Dobrovolska, Galyna and Finkelberg, Michael},
Title = {{Gaiotto-Witten superpotential and Whittaker D-modules on monopoles}},
Journal = {{ADVANCES IN MATHEMATICS}},
Year = {{2016}},
Volume = {{300}},
Pages = {{451-472}},
Month = {{SEP 10}},
Abstract = {{Let G be an almost simple simply connected group over C. For a positive
   element a of the coroot lattice of G let (Z) over circle (alpha) denote
   the space of maps from P-1 to the flag variety B of G sending infinity
   is an element of P-1 to a fixed point in B of degree a. This space is
   known to be isomorphic to the space of framed G-monopoles on R-3 with
   maximal symmetry breaking at infinity of charge alpha.
   In {[}6] a system of (etale, rational) coordinates on (Z) over circle
   (alpha) is introduced. In this note we compute various known structures
   on (Z) over circle (alpha) in terms of the above coordinates. As a
   byproduct we give a natural interpretation of the Gaiotto-Witten
   superpotential studied in {[}8] and relate it to the theory of Whittaker
   D-modules discussed in {[}9]. (C) 2016 Published by Elsevier Inc.}},
