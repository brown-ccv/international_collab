Title = {{Trends in adult body-mass index in 200 countries from 1975 to 2014: a
   pooled analysis of 1698 population-based measurement studies with 19.2
   million participants}},
Journal = {{LANCET}},
Year = {{2016}},
Volume = {{387}},
