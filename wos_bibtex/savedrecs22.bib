@article{ ISI:000370258400018,
Author = {Wang, Jiawen and Li, Bo and Cao, Yan-Ping and Feng, Xi-Qiao and Gao,
   Huajian},
Title = {{Wrinkling micropatterns regulated by a hard skin layer with a periodic
   stiffness distribution on a soft material}},
Journal = {{APPLIED PHYSICS LETTERS}},
Year = {{2016}},
Volume = {{108}},
Number = {{2}},
Month = {{JAN 11}},
Abstract = {{A wrinkling-based method is proposed to create various surface
   micropatterns regulated by a hardened skin layer with a periodic
   stiffness distribution on a soft material. It is shown that the surface
   patterns generated by this technique are controlled by three fundamental
   surface deformation modes that involve sinusoidal wrinkling, Euler
   buckling, and rigid rotation of the skin. Systematic experiments and a
   phase diagram validate the efficacy and robustness of the proposed
   method. (C) 2016 AIP Publishing LLC.}},
