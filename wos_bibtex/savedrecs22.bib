@article{ ISI:000386349400007,
Author = {Kuznetsova, E. I. and Fel'dman, E. B. and Feldman, D. E.},
Title = {{Magnus expansion paradoxes in the study of equilibrium magnetization and
   entanglement in multi-pulse spin locking}},
Journal = {{PHYSICS-USPEKHI}},
Year = {{2016}},
Volume = {{59}},
Number = {{6}},
Pages = {{577-582}},
Abstract = {{Divergence of the Magnus expansion leads to paradoxes in the spin
   dynamics of solid-state NMR and in quantum informatics. This review
   presents results on quasi-equilibrium magnetization in a system of
   dipole dipole (DD) coupled spins at times T-2 << t << T-1 rho in
   multiple-pulse spin locking (T-2 is the transverse spin relaxation time
   and Tip is the rotating-frame spin lattice relaxation time). It is shown
   how contradictions between the results obtained with the Magnus
   expansion and experimental data can be removed. Systems of two and three
   DD coupled spins in multi-pulse spin locking are considered, and the
   entanglement evolution is investigated using both the Magnus expansion
   and the exact solution. The critical temperature for an entangled state
   is also found.}},
