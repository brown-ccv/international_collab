Author = {Bokanowski, Olivier and Cheng, Yingda and Shu, Chi-Wang},
Title = {{CONVERGENCE OF DISCONTINUOUS GALERKIN SCHEMES FOR FRONT PROPAGATION WITH
   OBSTACLES}},
Journal = {{MATHEMATICS OF COMPUTATION}},
