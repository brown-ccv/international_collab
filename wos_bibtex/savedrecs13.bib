@article{ ISI:000379103200008,
Author = {Tate, Robyn L. and Perdices, Michael and Rosenkoetter, Ulrike and
   Shadish, William and Vohra, Sunita and Barlow, David H. and Horner,
   Robert and Kazdin, Alan and Kratochwill, Thomas and McDonald, Skye and
