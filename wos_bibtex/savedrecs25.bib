@article{ ISI:000364538800002,
Author = {Wang, Haijin and Shu, Chi-Wang and Zhang, Qing},
Title = {{Stability analysis and error estimates of local discontinuous Galerkin
   methods with implicit-explicit time-marching for nonlinear
   convection-diffusion problems}},
Journal = {{APPLIED MATHEMATICS AND COMPUTATION}},
Year = {{2016}},
Volume = {{272}},
Number = {{2}},
Pages = {{237-258}},
Month = {{JAN 1}},
Abstract = {{The main purpose of this paper is to analyze the stability and error
   estimates of the local discontinuous Galerkin (LDG) methods coupled with
   implicit-explicit (IMEX) time discretization schemes, for solving
   one-dimensional convection-diffusion equations with a nonlinear
   convection. Both Runge-Kutta and multi-step IMEX methods are considered.
   By the aid of the energy method, we show that the IMEX LDG schemes are
   unconditionally stable for the nonlinear problems, in the sense that the
   time-step tau is only required to be upper-bounded by a positive
   constant which depends on the flow velocity and the diffusion
   coefficient, but is independent of the mesh size h. We also give optimal
   error estimates for the IMEX LDG schemes, under the same temporal
   condition, if a monotone numerical flux is adopted for the convection.
   Numerical experiments are given to verify our main results. (C) 2015
   Elsevier Inc. All rights reserved.}},
