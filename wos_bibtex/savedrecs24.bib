@article{ ISI:000369591400017,
Author = {Sun, Xiaolian and Li, Dongguo and Guo, Shaojun and Zhu, Wenlei and Sun,
   Shouheng},
Title = {{Controlling core/shell Au/FePt nanoparticle electrocatalysis via
   changing the core size and shell thickness}},
Journal = {{NANOSCALE}},
Year = {{2016}},
Volume = {{8}},
Number = {{5}},
Pages = {{2626-2631}},
Abstract = {{Using a modified seed-mediated method, we synthesized core/shell Au/FePt
   nanoparticles (NPs) with Au sizes of 4, 7, and 9 nm and the FePt shell
   was controlled to have similar FePt compositions and 0.5, 1, and 2 nm
   thickness. We studied both core and shell effects on electrochemical and
   electrocatalytic properties of the Au/FePt NPs, and found that the Au
   core did change the redox chemistry of the FePt shell and promoted its
   electrochemical oxidation of methanol. The catalytic activity was
   dependent on the FePt thicknesses, but not much on the Au core sizes,
   and the 1 nm FePt shell was found to be the optimal thickness for
   catalyzing methanol oxidation in 0.1 M HClO4 + 0.1 M methanol, offering
   not only high activity (1.19 mA cm(-2) at 0.5 V vs. Ag/AgCl), but also
   enhanced stability. Our studies demonstrate a general approach to the
   design and tuning of shell catalysis in the core/shell structure to
   achieve optimal catalysis for important electrochemical reactions.}},
