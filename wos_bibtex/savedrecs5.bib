@article{ ISI:000384318800001,
Author = {Livingstone, Katherine M. and Celis-Morales, Carlos and Papandonatos,
   George D. and Erar, Bahar and Florez, Jose C. and Jablonski, Kathleen A.
   and Razquin, Cristina and Marti, Amelia and Heianza, Yoriko and Huang,
